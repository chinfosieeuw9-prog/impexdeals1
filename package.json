  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@tanstack/react-query": "^5.85.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.5",
    "dotenv": "^16.4.5",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "html-webpack-plugin": "^5.6.4",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.14.3",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "redis": "^4.7.1",
    "sharp": "^0.33.5",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:server": "node server.js",
    "dev:server": "nodemon --watch server.js --watch uploads --watch build/index.html --ext js,json,html server.js",
    "dev": "concurrently \"npm run dev:server\" \"npm start\"",
    "watch:build": "chokidar \"src/**/*.{ts,tsx,js,jsx,css}\" -i \"**/node_modules/**\" -c \"npm run build\"",
    "dev:prodstyle": "npm run build && concurrently \"npm run watch:build\" \"npm run dev:server\"",
    "clean:env": "powershell -ExecutionPolicy Bypass -File ./scripts/clean-env.ps1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.4"
  }
}
